   <columnConstraints>
      <ColumnConstraints hgrow="ALWAYS" minWidth="10.0" prefWidth="60.0" />
   </columnConstraints>
   <rowConstraints>
      <RowConstraints maxHeight="-Infinity" minHeight="10.0" prefHeight="30.0" vgrow="ALWAYS" />
      <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="ALWAYS" />
   </rowConstraints>
   <children>
      <HBox maxHeight="-Infinity" prefHeight="25.0" prefWidth="190.0">
         <children>
            <ToggleButton fx:id="scrollLockButton" mnemonicParsing="false" selected="true" styleClass=".play-button" text="Scroll Lock" />
         </children>
      </HBox>
   </children>
</GridPane>
